{
  "all": true,
  "include": ["app/**", "providers/**"],
  "exclude": [
    "node_modules/**",
    "build/**",
    "commands/**",
    "config/**",
    "contracts/**",
    "coverage/**",
    "dockerfiles/**",
    "docs/**",
    "start/**",
    "tests/**",
    "**/*.spec.ts",
    "test.ts",
    "env.ts",
    "server.ts",
    "**/models/",
    "app/**/handler.ts"
  ],
  "reporter": ["html", "lcov", "text", "text-summary", "json"],
  "lines": 75,
  "branches": 75,
  "statements": 75
}
